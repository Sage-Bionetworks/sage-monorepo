      "port": 21000,
      "driver": "PostgreSQL",
      "name": "bixarena-postgres",
      "database": "bixarena",
      "username": "postgres",
      "password": "changeme"
    },
    {
      "previewLimit": 50,
      "server": "openchallenges-postgres",
      "port": 8091,
      "driver": "PostgreSQL",
      "name": "openchallenges-organization-service",
      "database": "organization_service",
      "username": "organization_service",
      "password": "changeme"
    },
    {
      "previewLimit": 50,
