  "terminal.integrated.showExitAlert": false,
  "rewrap.wrappingColumn": 100,
  "jest.runMode": "on-demand",
  "nxConsole.generateAiAgentRules": true,
  "chat.tools.terminal.autoApprove": {
    "/^nx test .+/": true,
    "/^\\./gradlew (clean|compileJava|compileTestJava|test) .+/": true,
    "/^\\./gradlew\\s+:(?:[^:\\s]+:)+(clean|compileJava|compileTestJava|dependencies|test) .+/": true,
    "cat": true,
    "cd": true,
    "echo": true,
    "find": true,
    "git diff": true,
    "git log": true,
    "git show": true,
    "git status": true,
    "grep": true,
    "ls": true,
    "pwd": true,
    "rm": false,
    "rmdir": false,
    "kill": false,
    "curl": false,
    "wget": false,
    "eval": false,
    "chmod": false,
    "chown": false,
    "wc": true
  }
}
