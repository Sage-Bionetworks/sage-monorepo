{
  "name": "openchallenges-api-client-angular-deprecated",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/openchallenges/api-client-angular-deprecated/src",
  "projectType": "library",
  "prefix": "openchallenges",
  "targets": {
    "generate-sources": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "rm -fr src/lib/api",
          "rm -fr src/lib/model",
          "openapi-generator-cli generate -g typescript-angular -o src/lib -c openapi-generator.config.yaml -i ../api-spec/openapi.yaml"
        ],
        "cwd": "libs/openchallenges/api-client-angular-deprecated",
        "parallel": false
      }
    },
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/libs/openchallenges/api-client-angular-deprecated"],
      "options": {
        "project": "libs/openchallenges/api-client-angular-deprecated/ng-package.json",
        "updateBuildableProjectDepsInPackageJson": true
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/openchallenges/api-client-angular-deprecated/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/openchallenges/api-client-angular-deprecated/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/openchallenges/api-client-angular-deprecated"],
      "options": {
        "jestConfig": "libs/openchallenges/api-client-angular-deprecated/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/openchallenges/api-client-angular-deprecated/src/index.ts"]
      }
    },
    "lint-fix": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/openchallenges/api-client-angular-deprecated/src/index.ts"],
        "fix": true
      }
    }
  },
  "tags": []
}
