{
  "name": "explorers-charts",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/explorers/charts/src",
  "projectType": "library",
  "prefix": "sage",
  "tags": ["type:util", "scope:explorers", "language:typescript"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/explorers/charts",
        "main": "libs/explorers/charts/src/index.ts",
        "tsConfig": "libs/explorers/charts/tsconfig.lib.json",
        "assets": ["libs/explorers/charts/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "lint-fix": {
      "executor": "@nx/eslint:lint",
      "options": {
        "fix": true
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/explorers/charts/jest.config.ts"
      }
    }
  }
}
