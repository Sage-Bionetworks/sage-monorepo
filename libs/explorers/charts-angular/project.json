{
  "name": "explorers-charts-angular",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/explorers/charts-angular/src",
  "prefix": "sage",
  "tags": ["type:util", "scope:explorers", "language:typescript"],
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/explorers/charts-angular/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "lint-fix": {
      "executor": "@nx/eslint:lint",
      "options": {
        "fix": true
      }
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/explorers/charts-angular/.storybook",
        "browserTarget": "explorers-charts-angular:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/explorers-charts-angular",
        "configDir": "libs/explorers/charts-angular/.storybook",
        "browserTarget": "explorers-charts-angular:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "static-storybook": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "explorers-charts-angular:build-storybook",
        "staticFilePath": "dist/storybook/explorers-charts-angular"
      },
      "configurations": {
        "ci": {
          "buildTarget": "explorers-charts-angular:build-storybook:ci"
        }
      }
    }
  }
}
