{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/api-docs/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "redoc-cli bundle ../api-spec/openapi.yaml",
        "cwd": "libs/api-docs"
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "redoc-cli serve ../api-spec/openapi.yaml",
        "cwd": "libs/api-docs"
      }
    }
  },
  "tags": []
}
