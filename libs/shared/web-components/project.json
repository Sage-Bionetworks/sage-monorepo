{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/web-components/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shared/web-components/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared/web-components"],
      "options": {
        "jestConfig": "libs/shared/web-components/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:feature", "scope:shared"]
}
