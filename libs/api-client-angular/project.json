{
  "name": "api-client-angular",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/api-client-angular/src",
  "prefix": "challenge-registry",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/libs/api-client-angular"],
      "options": {
        "project": "libs/api-client-angular/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/api-client-angular/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/api-client-angular/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/api-client-angular"],
      "options": {
        "jestConfig": "libs/api-client-angular/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/api-client-angular/src/index.ts"]
      }
    },
    "lint-fix": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/api-client-angular/src/index.ts"],
        "fix": true
      }
    },
    "openapi-generate": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "rm -fr src/lib/*",
          "openapi-generator-cli generate",
          "echo 'TODO Format generated code'"
        ],
        "cwd": "libs/api-client-angular",
        "parallel": false
      }
    }
  },
  "tags": []
}
