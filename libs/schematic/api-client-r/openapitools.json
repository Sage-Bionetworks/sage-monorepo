{
  "$schema": "../../../node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.6.0",
    "generators": {
      "api-client-r": {
        "generatorName": "r",
        "inputSpec": "#{cwd}/../api-description/build/openapi.yaml",
        "output": "#{cwd}/src/lib/",
        "additionalProperties": {
          "disallowAdditionalPropertiesIfNotPresent": false,
          "library": "httr"
        }
      }
    }
  }
}
