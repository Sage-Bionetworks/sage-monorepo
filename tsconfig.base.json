        "libs/challenge-registry/challenge/src/index.ts"
      ],
      "@sagebionetworks/challenge-registry/challenge-search": [
        "libs/challenge-registry/challenge-search/src/index.ts"
      ],
      "@sagebionetworks/challenge-registry/config": ["libs/challenge-registry/config/src/index.ts"],
      "@sagebionetworks/challenge-registry/home": ["libs/challenge-registry/home/src/index.ts"],
      "@sagebionetworks/challenge-registry/login": ["libs/challenge-registry/login/src/index.ts"],
      "@sagebionetworks/challenge-registry/not-found": [
        "libs/challenge-registry/not-found/src/index.ts"
