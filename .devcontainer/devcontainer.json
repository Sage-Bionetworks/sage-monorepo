{
  "name": "Challenge",
  "image": "sagebionetworks/challenge-devcontainer:6c35430",

  "features": {
    "ghcr.io/devcontainers/features/docker-in-docker:2.0.0": {
      "version": "20.10.21"
    }
  },

  "settings": {},

  "extensions": [
    "1000ch.svgo",
    "Angular.ng-template",
    "dbaeumer.vscode-eslint",
    "emeraldwalk.RunOnSave",
    "esbenp.prettier-vscode",
    "firsttris.vscode-jest-runner",
    "GitHub.vscode-pull-request-github",
    "Gruntfuggly.todo-tree",
    "humao.rest-client",
    "mhutchie.git-graph",
    "mongodb.mongodb-vscode",
    "ms-python.python",
    "mtxr.sqltools-driver-mysql",
    "mtxr.sqltools-driver-pg",
    "mtxr.sqltools",
    "nrwl.angular-console",
    "Pivotal.vscode-boot-dev-pack",
    "ritwickdey.LiveServer",
    "shengchen.vscode-checkstyle",
    "stkb.rewrap",
    "vscjava.vscode-gradle",
    "vscjava.vscode-java-pack",
    "webhint.vscode-webhint"
  ],

  "forwardPorts": [
    3306,
    4200,
    4201,
    4211,
    5000,
    5044,
    5432,
    5601,
    8080,
    8081,
    8082,
    8083,
    8084,
    8090,
    8091,
    8092,
    9200,
    9300,
    9600,
    27017
  ],

  "portsAttributes": {
    "3306": {
      "label": "challenge-mariadb",
      "onAutoForward": "silent"
    },
    "4200": {
      "label": "challenge-registry",
      "onAutoForward": "silent"
    },
    "4201": {
      "label": "challenge-platform",
      "onAutoForward": "silent"
    },
    "4211": {
      "label": "nx graph",
      "onAutoForward": "silent"
    },
    "5000": {
      "label": "challenge-logstash (TCP input)",
      "onAutoForward": "silent"
    },
    "5044": {
      "label": "challenge-logstash (Beats input)",
      "onAutoForward": "silent"
    },
    "5432": {
      "label": "challenge-postgresql",
      "onAutoForward": "silent"
    },
    "5601": {
      "label": "challenge-kibana",
      "onAutoForward": "silent"
    },
    "8080": {
      "label": "challenge-keycloak",
      "onAutoForward": "silent"
    },
    "8081": {
      "label": "challenge-service-registry",
      "onAutoForward": "silent"
    },
    "8082": {
      "label": "challenge-api-gateway",
      "onAutoForward": "silent"
    },
    "8083": {
      "label": "challenge-user-service",
      "onAutoForward": "silent"
    },
    "8084": {
      "label": "challenge-organization-service",
      "onAutoForward": "silent"
    },
    "8090": {
      "label": "challenge-config-service",
      "onAutoForward": "silent"
    },
    "8091": {
      "label": "challenge-auth-service",
      "onAutoForward": "silent"
    },
    "8092": {
      "label": "challenge-core-service",
      "onAutoForward": "silent"
    },
    "9200": {
      "label": "challenge-elasticsearch (HTTP)",
      "onAutoForward": "silent"
    },
    "9300": {
      "label": "challenge-elasticsearch (TCP transport)",
      "onAutoForward": "silent"
    },
    "9600": {
      "label": "challenge-logstash (monitoring API)",
      "onAutoForward": "silent"
    },
    "27017": {
      "label": "challenge-mongodb",
      "onAutoForward": "silent"
    }
  },

	"remoteUser": "vscode",
  "shutdownAction": "stopContainer",
  "runArgs": ["--name", "challenge_devcontainer"]
}
