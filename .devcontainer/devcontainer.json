{
  "name": "Sage",
  "image": "sagebionetworks/sage-devcontainer:3de9f17",

  "features": {
    "ghcr.io/devcontainers/features/docker-in-docker:2.0.0": {
      "version": "20.10.21"
    }
  },

  "settings": {},

  "extensions": [
    "1000ch.svgo",
    "Angular.ng-template",
    "dbaeumer.vscode-eslint",
    "emeraldwalk.RunOnSave",
    "esbenp.prettier-vscode",
    "firsttris.vscode-jest-runner",
    "GitHub.vscode-pull-request-github",
    "Gruntfuggly.todo-tree",
    "humao.rest-client",
    "mhutchie.git-graph",
    "mongodb.mongodb-vscode",
    "ms-python.python",
    "mtxr.sqltools-driver-mysql",
    "mtxr.sqltools-driver-pg",
    "mtxr.sqltools",
    "nrwl.angular-console",
    "Pivotal.vscode-boot-dev-pack",
    "ritwickdey.LiveServer",
    "shengchen.vscode-checkstyle",
    "stkb.rewrap",
    "vscjava.vscode-gradle",
    "vscjava.vscode-java-pack",
    "webhint.vscode-webhint"
  ],

  "forwardPorts": [
    3306,
    4200,
    4211,
    5432,
    5601,
    7010,
    7080,
    7200,
    7888,
    8010,
    8020,
    8021,
    8080,
    8081,
    8082,
    8083,
    8084,
    8085,
    8090,
    8091,
    8092,
    9200,
    27017
  ],

  "portsAttributes": {
    "3306": {
      "label": "challenge-registry-mariadb",
      "onAutoForward": "silent"
    },
    "4200": {
      "label": "challenge-registry-app",
      "onAutoForward": "silent"
    },
    "4211": {
      "label": "nx graph",
      "onAutoForward": "silent"
    },
    "5432": {
      "label": "challenge-registry-postgres",
      "onAutoForward": "silent"
    },
    "5601": {
      "label": "challenge-registry-opensearch-dashboards",
      "onAutoForward": "silent"
    },
    "7010": {
      "label": "schematic-api-docs",
      "onAutoForward": "silent"
    },
    "7080": {
      "label": "schematic-api",
      "onAutoForward": "silent"
    },
    "7200": {
      "label": "schematic-app",
      "onAutoForward": "silent"
    },
    "7888": {
      "label": "schematic-notebook",
      "onAutoForward": "silent"
    },
    "8010": {
      "label": "challenge-registry-api-docs",
      "onAutoForward": "silent"
    },
    "8020": {
      "label": "challenge-registry-rabbitmq",
      "onAutoForward": "silent"
    },
    "8021": {
      "label": "challenge-registry-rabbitmq-ui",
      "onAutoForward": "silent"
    },
    "8080": {
      "label": "challenge-registry-keycloak",
      "onAutoForward": "silent"
    },
    "8081": {
      "label": "challenge-registry-service-registry",
      "onAutoForward": "silent"
    },
    "8082": {
      "label": "challenge-registry-api-gateway",
      "onAutoForward": "silent"
    },
    "8083": {
      "label": "challenge-registry-user-service",
      "onAutoForward": "silent"
    },
    "8084": {
      "label": "challenge-registry-organization-service",
      "onAutoForward": "silent"
    },
    "8085": {
      "label": "challenge-registry-challenge-service",
      "onAutoForward": "silent"
    },
    "8090": {
      "label": "challenge-registry-config-service",
      "onAutoForward": "silent"
    },
    "8091": {
      "label": "challenge-registry-auth-service",
      "onAutoForward": "silent"
    },
    "8092": {
      "label": "challenge-registry-core-service",
      "onAutoForward": "silent"
    },
    "9200": {
      "label": "challenge-registry-elasticsearch",
      "onAutoForward": "silent"
    },
    "27017": {
      "label": "challenge-registry-mongo",
      "onAutoForward": "silent"
    }
  },

	"remoteUser": "vscode",
  "shutdownAction": "stopContainer",
  "runArgs": ["--name", "sage_devcontainer"]
}
