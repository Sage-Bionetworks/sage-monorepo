query FeatureValuesByTag(
  $dataSet: [String!]
  $related: [String!]
  $tag: [String!]
  $feature: [String!]
  $sample: [String!]
) {
  featuresByTag(
    dataSet: $dataSet
    related: $related
    tag: $tag
    feature: $feature
    sample: $sample
  ){
    tag
    display
    color
    characteristics
    features{
      samples {
        name
        value
      }
    }
  }
}
