{
  "root": "apps/movie-api",
  "sourceRoot": "apps/movie-api/src",
  "projectType": "application",
  "targets": {
    "prepare": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "shx cp .env.example .env",
        "cwd": "apps/movie-api"
      }
    },
    "build": {
      "executor": "@nxrocks/nx-spring-boot:build",
      "options": {
        "root": "apps/movie-api"
      }
    },
    "test": {
      "executor": "@nxrocks/nx-spring-boot:test",
      "options": {
        "root": "apps/movie-api"
      }
    },
    "clean": {
      "executor": "@nxrocks/nx-spring-boot:clean",
      "options": {
        "root": "apps/movie-api"
      }
    },
    "format": {
      "executor": "@nxrocks/nx-spring-boot:format",
      "options": {
        "root": "apps/movie-api"
      }
    },
    "format-check": {
      "executor": "@nxrocks/nx-spring-boot:format-check",
      "options": {
        "root": "apps/movie-api"
      }
    },
    "run": {
      "executor": "@nxrocks/nx-spring-boot:run",
      "options": {
        "root": "apps/movie-api"
      }
    },
    "serve": {
      "executor": "@nxrocks/nx-spring-boot:serve",
      "options": {
        "root": "apps/movie-api",
        "args": ["-P", "dev"]
      }
    },
    "build-image": {
      "executor": "@nx-tools/nx-docker:build",
      "options": {
        "context": "apps/movie-api",
        "push": false,
        "tags": ["sagebionetworks/challenge-registry-movie-api:latest"]
      }
    },
    "serve-image": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "docker compose up",
        "cwd": "apps/movie-api"
      }
    }
  },
  "tags": []
}
