{
  "$schema": "../../../node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.14.0",
    "generators": {
      "agora-api-next": {
        "generatorName": "spring",
        "inputSpec": "#{cwd}/../../../libs/agora/api-description/openapi/api-next-service.openapi.yaml",
        "output": "#{cwd}",
        "additionalProperties": {
          "apiPackage": "org.sagebionetworks.agora.api.next.api",
          "basePackage": "org.sagebionetworks.agora.api.next",
          "configPackage": "org.sagebionetworks.agora.api.next.configuration",
          "delegatePattern": true,
          "hideGenerationTimestamp": true,
          "library": "spring-boot",
          "modelNameSuffix": "Dto",
          "modelPackage": "org.sagebionetworks.agora.api.next.model.dto",
          "useTags": true,
          "openApiNullable": false,
          "useSpringBoot3": true,
          "generateBuilders": true
        }
      }
    }
  }
}
