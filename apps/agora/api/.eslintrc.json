{
  "extends": ["../../../.eslintrc.json"],
  "ignorePatterns": ["!**/*", "**/*.config.js", "**/*.config.ts"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "tailwindcss/no-custom-classname": "off"
      }
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
