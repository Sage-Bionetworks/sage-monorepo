{
  "$schema": "../../../node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.12.0",
    "generators": {
      "agora-gene-api": {
        "generatorName": "spring",
        "inputSpec": "#{cwd}/../../../libs/agora/api-description/openapi/gene-api.openapi.yaml",
        "output": "#{cwd}",
        "additionalProperties": {
          "apiPackage": "org.sagebionetworks.agora.gene.api.api",
          "basePackage": "org.sagebionetworks.agora.gene.api",
          "configPackage": "org.sagebionetworks.agora.gene.api.configuration",
          "delegatePattern": true,
          "hideGenerationTimestamp": true,
          "library": "spring-boot",
          "modelNameSuffix": "Dto",
          "modelPackage": "org.sagebionetworks.agora.gene.api.model.dto",
          "useTags": true,
          "openApiNullable": false,
          "useSpringBoot3": true,
          "generateBuilders": true
        }
      }
    }
  }
}
