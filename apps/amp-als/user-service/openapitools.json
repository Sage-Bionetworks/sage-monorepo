{
  "$schema": "../../../node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.12.0",
    "generators": {
      "amp-als-user-service": {
        "generatorName": "spring",
        "inputSpec": "#{cwd}/../../../libs/amp-als/api-description/openapi/user.openapi.yaml",
        "output": "#{cwd}",
        "additionalProperties": {
          "apiPackage": "org.sagebionetworks.amp.als.user.service.api",
          "basePackage": "org.sagebionetworks.amp.als.user.service",
          "configPackage": "org.sagebionetworks.amp.als.user.service.configuration",
          "delegatePattern": true,
          "hideGenerationTimestamp": true,
          "library": "spring-boot",
          "modelNameSuffix": "Dto",
          "modelPackage": "org.sagebionetworks.amp.als.user.service.model.dto",
          "useTags": true,
          "openApiNullable": false,
          "useSpringBoot3": true,
          "generateBuilders": true
        }
      }
    }
  }
}
