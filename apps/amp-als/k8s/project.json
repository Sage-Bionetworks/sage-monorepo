{
  "name": "amp-als-k8s",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "targets": {
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./deploy.sh",
        "cwd": "{projectRoot}"
      }
    },
    "destroy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "./deploy.sh --cleanup",
        "cwd": "{projectRoot}"
      }
    }
  },
  "tags": ["type:k8s"]
}
