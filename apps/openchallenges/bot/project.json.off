{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/challenge-bot/src",
  "projectType": "application",
  "targets": {
    "build-watch": {
      "executor": "@nx/workspace:run-commands",
      "options": {
        "command": "tsc -p tsconfig.app.json --watch",
        "cwd": "apps/challenge-bot"
      }
    },
    "prepare": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn install --immutable",
        "cwd": "apps/challenge-bot"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nodemon src/run-challenge-app.ts",
        "cwd": "apps/challenge-bot"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "serverless deploy",
        "cwd": "apps/challenge-bot"
      }
    },
    "deploy-offline": {
      "executor": "nx:run-commands",
      "options": {
        "command": "serverless offline",
        "cwd": "apps/challenge-bot"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/challenge-bot/src/**/*.ts"]
      }
    },
    "lint-fix": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/challenge-bot/src/**/*.ts"],
        "fix": true
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["coverage/apps/challenge-bot"],
      "options": {
        "jestConfig": "apps/challenge-bot/jest.config.ts",
        "passWithNoTests": true,
        "showConfig": false
      }
    }
  },
  "tags": ["type:app", "scope:admin"],
  "name": "challenge-bot"
}
