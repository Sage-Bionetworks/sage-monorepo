{
  "$schema": "../../../node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.2.1",
    "generators": {
      "openchallenges-challenge-service": {
        "config": "templates/config.yaml",
        "generatorName": "spring",
        "inputSpec": "#{cwd}/../../../libs/openchallenges/api-description/build/challenge.openapi.yaml",
        "output": "#{cwd}",
        "templateDir": "templates",
        "additionalProperties": {
          "apiPackage": "org.sagebionetworks.openchallenges.challenge.service.api",
          "basePackage": "org.sagebionetworks.openchallenges.challenge.service",
          "configPackage": "org.sagebionetworks.openchallenges.challenge.service.configuration",
          "delegatePattern": true,
          "hideGenerationTimestamp": true,
          "library": "spring-boot",
          "modelNameSuffix": "Dto",
          "modelPackage": "org.sagebionetworks.openchallenges.challenge.service.model.dto",
          "useTags": true,
          "openApiNullable": false
        }
      }
    }
  }
}
