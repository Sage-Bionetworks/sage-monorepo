{
  "$schema": "../../node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.1.0",
    "generators": {
      "challenge-user-service": {
        "config": "templates/config.yaml",
        "generatorName": "spring",
        "inputSpec": "#{cwd}/../../libs/api-spec/build/user.openapi.yaml",
        "output": "#{cwd}",
        "templateDir": "templates",
        "additionalProperties": {
          "apiPackage": "org.sagebionetworks.challenge.api",
          "basePackage": "org.sagebionetworks.challenge",
          "configPackage": "org.sagebionetworks.challenge.configuration",
          "delegatePattern": true,
          "hideGenerationTimestamp": true,
          "library": "spring-boot",
          "modelNameSuffix": "Dto",
          "modelPackage": "org.sagebionetworks.challenge.model.dto",
          "useTags": true
        }
      }
    }
  }
}
